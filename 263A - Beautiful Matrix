#include<bits/stdc++.h>
using namespace std;
int main()
{
    int i,j,x,y,m;
    //vector<int> m[5];
    for(i=0;i<5;i++)
    {
        for(j=0;j<5;j++)
        {
            cin>>m;
            if(m == 1)
            {
                x=i;
                y=j;
            }
        }
    }
    cout<<abs(x-2)+abs(y-2)<<endl;


}



