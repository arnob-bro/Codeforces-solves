#include<bits/stdc++.h>
using namespace std;
int main()
{
    string s;
    cin>>s;
    int ss = s.size();
    for(int i=0;i<ss-6;i++)
    {
        if(s[i]==s[i+1] && s[i+1]==s[i+2] && s[i+2]==s[i+3] && s[i+3]==s[i+4] && s[i+4]==s[i+5] && s[i+5]==s[i+6] && s[i+6]==s[i])
        {
            cout<<"YES"<<endl;
            return 0;
        }

    }
    cout<<"NO"<<endl;
}
